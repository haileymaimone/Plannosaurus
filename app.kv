        text_color: 0, 0, 0, 1
        on_release:
            app.showAddSticker_dialog(the_event_item)
			

    IconRightWidget:
        icon: 'trash-can-outline'
        theme_text_color: "Custom"
        text_color: 0, 0, 0, 1
        on_release:
