<DialogContent>:
	orientation: "vertical"
	spacing: "10dp"
	size_hint: 1, None
	height: "130dp"
	
	GridLayout:
		rows: 1
		
		MDTextField:
			id: task_text
			hint_text: "Add To-Do"
			pos_hint: {"center_y": .4}
			max_text_length: 50
			on_text_validate: (app.add_todo(task_text, date_text.text), app.close_dialog())
			
	MDLabel:
		spacing: '10dp'
		id: date_text
		
	BoxLayout:
		orientation: 'horizontal'
		
		MDRaisedButton:
			text: 'SAVE'
			on_release: (app.add_todo(task_text, date_text.text), app.postTodo(), app.close_todolist_dialog())
			
		
		MDFlatButton:
			text: 'CANCEL'
			on_release: app.close_todolist_dialog()
			
